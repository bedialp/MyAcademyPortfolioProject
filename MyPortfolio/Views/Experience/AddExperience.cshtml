@model MyPortfolio.Models.TblExperiences
@{
    ViewBag.Title = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<br />
<br />
<h2>Yeni Deneyim Ekleme Sayfası</h2>

<form method="post">
    <input type="hidden" name="ExperienceId"/>
    <label>Başlangıç Yılı</label>
    <input type="text" name="StartYear" class="form-control" />
    <br />
    <label>Bitiş Yılı</label>
    <input type="text" name="EndYear" class="form-control" />
    <br />
    <label>Başlık</label>
    <input type="text" name="Title" class="form-control" />
    <br />
    <label>Açıklama</label>
    <input type="text" name="Description" class="form-control" />
    <br />
    <label>Şirket</label>
    <input type="text" name="Company" class="form-control" />
    <br />
    <label>Konum</label>
    <input type="text" name="Location" class="form-control" />
    <br />


    <button class="btn btn-outline-primary">Kaydet</button>

</form>